{
  "name": "cure-ui",
  "version": "0.0.0",
  "author": "master1lan",
  "description": "Simple implementation of react component library",
  "type": "module",
  "main": "./lib/index.umd.js",
  "keywords": [
    "react",
    "react-component"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/master1lan/cure-ui",
    "directory": "src"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/master1lan/cure-ui/issues"
  },
  "homepage": "todo",
  "scripts": {
    "dev:src": "vite",
    "dev:dumi": "run-s lib:build dumi",
    "lib:build": "vite build",
    "lib:types": "tsc -p tsconfig.build.json",
    "lib:doc": "cross-env APP_ROOT=dumi dumi build",
    "lib": "run-s lib:*",
    "preview": "vite preview",
    "dumi": "cross-env APP_ROOT=dumi dumi dev",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "files": [
    "lib"
  ],
  "module": "./lib/index.es.js",
  "types": "./lib/index.d.ts",
  "typings": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "module": "./lib/index.es.js",
      "import": "./lib/index.es.js",
      "require": "./lib/index.umd.js",
      "default": "./lib/index.umd.js"
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/ui": "^0.28.4",
    "cross-env": "^7.0.3",
    "dumi": "^2.1.4",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "typescript": "^4.9.3",
    "vite": "^4.1.1",
    "vitest": "^0.28.4",
    "webpack": ">=5.20.0 <6.0.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "nanoid": "^4.0.0"
  }
}
